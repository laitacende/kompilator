VAR 
	a
BEGIN
	a ASSIGN 4 PLUS 4;
	
	IF a NEQ 4 THEN
		WRITE 0;
	ELSE
		WRITE 1;
	ENDIF
	
	IF a NEQ 8 THEN
		WRITE 2;
	ELSE
		WRITE 3;
	ENDIF
	
	IF a EQ 10 THEN 
		WRITE 4;
	ELSE
		WRITE 5;
	ENDIF
	
	IF a EQ 8 THEN 
		WRITE 6;
	ELSE
		WRITE 7;
	ENDIF
	
	IF a LE 10 THEN 
		WRITE 8;
	ELSE
		WRITE 9;
	ENDIF
	
	IF a LE 5 THEN 
		WRITE 10;
	ELSE
		WRITE 11;
	ENDIF
	
	IF a LEQ 8 THEN 
		WRITE 12;
	ELSE
		WRITE 13;
	ENDIF
	
	IF a LEQ 15 THEN 
		WRITE 14;
	ELSE
		WRITE 15;
	ENDIF
	
	IF a GE 1 THEN 
		WRITE 16;
	ELSE
		WRITE 17;
	ENDIF
	
	IF a GE 9 THEN 
		WRITE 18;
	ELSE
		WRITE 19;
	ENDIF
	
	IF a GEQ 8 THEN 
		WRITE 20;
	ELSE
		WRITE 21;
	ENDIF
	
	IF a GEQ 50 THEN 
		WRITE 22;
	ELSE
		WRITE 23;
	ENDIF
	
	
END
