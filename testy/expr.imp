VAR
	a, b
BEGIN
	a ASSIGN 1 PLUS 9;
	WRITE a;
	b ASSIGN 3;
	WRITE b;
	a ASSIGN a PLUS 3;
	WRITE a;
	b ASSIGN 4;
	WRITE b;
	a ASSIGN b MINUS 1;
	WRITE a;
	a ASSIGN 3 TIMES 5;
	WRITE a;
	b ASSIGN 6;
	WRITE b;
	a ASSIGN 1 MINUS 1;
	WRITE a;
	a ASSIGN -1 PLUS 2;
	WRITE a;
	a ASSIGN -9 PLUS 5;
	WRITE a;
END
