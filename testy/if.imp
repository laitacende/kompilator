VAR
	a, b, c[0:2]
BEGIN
	a ASSIGN 8;
	b ASSIGN 2;
	c[0] ASSIGN 8;
	c[1] ASSIGN 2;
	
	IF c[0] EQ  8 THEN
		WRITE 1;
	ENDIF
	
	IF c[0] EQ c[1] THEN
		WRITE 2;
	ENDIF
	
	IF c[0] NEQ c[1] THEN
		WRITE 3;
	ENDIF
	
	IF c[0] NEQ c[1] THEN
		WRITE 4;
	ENDIF
	
	IF 4 LE c[0] THEN
		WRITE 5;
	ENDIF
	
	IF 4 LE c[1] THEN
		WRITE 6;
	ENDIF
	
	IF 5 LE 4 THEN
		WRITE -1;
	ENDIF
	
	IF 4 LE 5 THEN
		WRITE -2;
	ENDIF
	
	IF 4 LE 4 THEN
		WRITE -3;
	ENDIF
	
	IF c[0] GE c[1] THEN
		WRITE 7;
	ENDIF
	
	IF c[0] GE 10 THEN
		WRITE 8;
	ENDIF
	
	IF c[0] LEQ 8 THEN 
		WRITE 9;
	ENDIF
	
	IF c[0] LEQ 1 THEN 
		WRITE 10;
	ENDIF
	
	IF c[1] GEQ 2 THEN 
		WRITE 11;
	ENDIF
	
	IF c[1] GEQ c[0] THEN
		WRITE 12;
	ENDIF
	

	 
END
